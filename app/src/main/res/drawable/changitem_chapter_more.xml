<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <stroke
                android:width="0.5dp"
                android:color="@color/gray_c7c8cb" />

            <solid android:color="@color/white" />


        </shape>
    </item>
    <item
        android:bottom="0.5dp"
        android:left="0.5dp"
        android:right="0.5dp"
        android:top="0.5dp">
        <shape>
            <stroke
                android:dashGap="5dp"
                android:dashWidth="5dp"
                android:width="0.5dp"
                android:color="@color/gray_c7c8cb" />

            <solid android:color="@color/white" />


        </shape>
    </item>

</layer-list>