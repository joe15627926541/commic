<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical">



    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <include layout="@layout/home_head"/>

            <include layout="@layout/item_home"/>

            <include layout="@layout/layout_item_top"/>
            <include layout="@layout/layout_item_update"/>
            <include layout="@layout/layout_item_end_commic"/>
            <include layout="@layout/layout_item_end_must_see"/>
            <include layout="@layout/layout_item_end_you_like"/>


        </LinearLayout>


    </androidx.core.widget.NestedScrollView>


</LinearLayout>