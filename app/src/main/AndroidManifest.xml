        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/mine_head_icon"
        android:supportsRtl="true"
        android:theme="@style/NMTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:launchMode="singleTask"
            android:name=".MainActivity"
            android:windowSoftInputMode="adjustPan">

        </activity>

        <activity
            android:name=".SplashActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".read.DetailComicActivity" />
        <activity android:name=".read.ReadActivity" />
        <activity android:name=".me.WelfareCneterActivity" />
        <activity android:name=".me.ComplanitActivity" />
        <activity android:name=".me.FeedBackActivity" />
        <activity android:name=".me.BindPhoneActivity" />
        <activity android:name=".me.PasswordUpdateActivity" />
        <activity android:name=".me.ReadHistoryActivity" />
        <activity android:name=".me.OrderActivity" />
        <activity android:name=".me.FindAccountActivity" />
        <activity android:name=".me.ChangeAccountActivity" />
        <activity android:name=".me.TopupActivity" />
        <activity android:name="com.luoshunkeji.TestActivity">


        </activity>
        <activity
            android:name=".MyWebView"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait">

        </activity>


